# Environment Variables Template
# Copy this file to .env and fill in your values
# DO NOT commit .env to Git - it contains sensitive data

# ============================================
# GitLab Configuration
# ============================================
GITLAB_URL=https://gitlab.com
GITLAB_TOKEN=your-gitlab-token-here
GITLAB_PROJECT_ID=your-project-id-here
GITLAB_PIPELINE_ID=optional-pipeline-id

# ============================================
# Jira Configuration
# ============================================
JIRA_URL=https://your-company.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token-here
JIRA_PROJECT_KEY=PROJ

# ============================================
# Postman Configuration
# ============================================
POSTMAN_API_KEY=your-postman-api-key-here
POSTMAN_WORKSPACE_ID=your-workspace-id-here

# ============================================
# Confluence Configuration
# ============================================
CONFLUENCE_URL=https://your-company.atlassian.net/wiki/home

# ============================================
# GitHub Configuration (optional)
# ============================================
GITHUB_TOKEN=your-github-token-here

